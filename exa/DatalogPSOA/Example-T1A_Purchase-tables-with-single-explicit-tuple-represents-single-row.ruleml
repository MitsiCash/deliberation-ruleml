<!-- Explicit tuple (added in Deliberation PSOA RuleML) -->
<Atom xmlns="http://ruleml.org/spec" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://ruleml.org/spec http://deliberation.ruleml.org/1.03-psoa/xsd/datalogPSOA.xsd">
  <Rel>purchase</Rel>  <!-- purchase(+[John Mary Fido]) -->
  <tupdep>
    <Tuple>
      <Ind>John</Ind>
      <Ind>Mary</Ind>
      <Ind>Fido</Ind>
    </Tuple>
  </tupdep>
</Atom>
